class Solution {
    public int search(int[] nums, int target) {
        int size=nums.length;
        int l=0;
        int h=size-1;
        int mid;
        Arrays.sort(nums);
        while(l<=h){
            mid=(l+h)/2;
            if(nums[mid]==target){
                return mid;
            }
            else if(nums[mid]<target){
                l=mid+1;
            }
            else{
                h=mid-1;
            }
        }
        return -1;
    }
}
