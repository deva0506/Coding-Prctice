public class Main
{
    public static int ls(int arr[],int t){
        int n=arr.length;
        for(int i=0;i<n;i++){
            if(arr[i]==t){
                return i;
            }
        }
        return -1;
    }
	public static void main(String[] args) {
		int arr[]={1,2,3,4,5,2,3,4};
		int t=5;
		System.out.println(ls(arr,t));
	}
}
